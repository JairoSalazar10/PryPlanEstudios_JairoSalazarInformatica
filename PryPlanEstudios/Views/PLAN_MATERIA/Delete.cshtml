@model CapaNegocio.Entities.PLAN_MATERIA

@{
    ViewBag.Title = "Delete";
    Layout = null;
}

<h4>El registro se eliminará, desea continuar?</h4>
<br />
<div>
   
    <dl class="dl-horizontal">
        
        <dt>
            Código Plan
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PLAN.PLA_CODIGO)
        </dd>

        <dt>
            Plan
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PLAN.PLA_NOMBRE)
        </dd>
        <dt>
            Código Materia
        </dt>

        <dd>
            @Html.DisplayFor(model => model.MATERIA.MAT_CODIGO)
        </dd>
        <dt>
            Materia
        </dt>

        <dd>
            @Html.DisplayFor(model => model.MATERIA.MAT_NOMBRE)
        </dd>
        <dt>
            Nivel
        </dt>

        <dd>
            @Html.DisplayFor(model => model.MATERIA.MAT_NIVEL)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color text-center">
            <input type="submit" value="Eliminar" class="btn btn-danger" />
        </div>
    }
</div>
